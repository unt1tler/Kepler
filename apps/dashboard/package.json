{
  "name": "@kepler/dashboard",
  "version": "6.0.0",
  "description": "Dashboard for Kepler built on Next.js and Tailwind.css",
  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "build": "next build",
    "docker:build": "cd ../.. & docker build -t kepler/dashboard:latest -f ./apps/dashboard/Dockerfile .",
    "deploy": "pnpm run build && pnpm run start",
    "format": "prettier . --write --cache --ignore-unknown --plugin=prettier-plugin-tailwindcss",
    "format:check": "prettier . --check --cache",
    "analyze": "ANALYZE=true next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
  },
  "dependencies": {
    "@headlessui/react": "1.7.19",
    "@headlessui/tailwindcss": "0.2.0",
    "@kepler/config": "workspace:*",
    "@kepler/database": "workspace:*",
    "@kepler/util": "workspace:*",
    "@mdx-js/loader": "3.0.1",
    "@mdx-js/react": "3.0.1",
    "@next-auth/prisma-adapter": "1.0.7",
    "@next/mdx": "14.2.2",
    "@riskymh/discord-markdown": "3.0.0",
    "@tippyjs/react": "4.2.6",
    "@vercel/analytics": "1.2.2",
    "@vercel/speed-insights": "1.0.10",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "cobe": "0.6.3",
    "discord-api-types": "0.37.81",
    "geist": "1.3.0",
    "globby": "14.0.1",
    "js-file-download": "0.4.12",
    "json-2-csv": "5.5.1",
    "lucide-react": "0.373.0",
    "mini-svg-data-uri": "1.4.4",
    "next": "14.2.10",
    "next-auth": "4.24.7",
    "next-nprogress-bar": "2.3.11",
    "react": "18.2.0",
    "react-colorful": "5.6.1",
    "react-dom": "18.2.0",
    "react-infinite-scroller": "1.2.6",
    "react-spring": "9.7.3",
    "react-table": "7.8.0",
    "react-wrap-balancer": "1.1.0",
    "recharts": "2.12.6",
    "server-only": "0.0.1",
    "sharp": "0.33.3",
    "sonner": "1.4.41",
    "validator": "13.11.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "14.2.2",
    "@prisma/nextjs-monorepo-workaround-plugin": "5.13.0",
    "@tailwindcss/typography": "0.5.12",
    "autoprefixer": "10.4.19",
    "eslint-config-next": "14.2.2",
    "postcss": "8.4.38",
    "prettier-plugin-tailwindcss": "0.5.14",
    "tailwind-gradient-mask-image": "1.2.0",
    "tailwind-merge": "2.3.0",
    "tailwindcss": "3.4.3",
    "tailwindcss-text-fill": "0.2.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "browserslist": [
    ">0.3%",
    "not dead",
    "not op_mini all"
  ],
  "homepage": "https://keplerbot.xyz"
}
